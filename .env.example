# --- Discord ---
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_application_client_id
# For fast iteration, deploy commands to a single guild (optional)
DEV_GUILD_ID=your_test_guild_id

# --- Postgres (either DATABASE_URL or individual PG_* vars) ---
# DATABASE_URL=postgres://user:password@host:5432/dbname
PG_USER=postgres
PG_PASSWORD=
PG_HOST=localhost
PG_DATABASE=discordbotdb
PG_PORT=5432
# Set to 'require' if using managed Postgres that enforces TLS
PGSSL=

# --- Monitoring ---
ERROR_WEBHOOK_URL=
LOG_LEVEL=info
